{
    "name": "saeedvir/shopping-cart",
    "description": "A comprehensive shopping cart package for Laravel 11/12 with tax calculation, discounts, coupons, and flexible storage options",
    "keywords": [
        "laravel",
        "shopping-cart",
        "e-commerce",
        "cart",
        "laravel-11",
        "laravel-12",
        "tax-calculation",
        "discounts",
        "coupons"
    ],
    "homepage": "https://github.com/saeedvir/shopping-cart",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Saeedvir",
            "email": "saeed.es91@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/saeedvir/shopping-cart/issues",
        "source": "https://github.com/saeedvir/shopping-cart"
    },
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/database": "^11.0|^12.0",
        "illuminate/session": "^11.0|^12.0",
        "illuminate/cache": "^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Saeedvir\\ShoppingCart\\": "src/"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Saeedvir\\ShoppingCart\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Saeedvir\\ShoppingCart\\ShoppingCartServiceProvider"
            ],
            "aliases": {
                "Cart": "Saeedvir\\ShoppingCart\\Facades\\Cart"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
